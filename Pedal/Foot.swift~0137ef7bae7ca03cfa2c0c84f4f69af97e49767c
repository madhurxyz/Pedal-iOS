import Foundation
import UIKit
struct Foot {
    var pulse: Pulse
    var sensitivity: Sensitivity
    var palm: Palm
    var ankle: Ankle
    var standing: Standing
    var side: Bool
    
    init(pulse: Pulse, sensitivity: Sensitivity, palm: Palm, ankle: Ankle, standing: Standing, side: Bool){
        self.pulse = pulse
        self.sensitivity = sensitivity
        self.palm = palm
        self.ankle = ankle
        self.standing = standing
        self.side = side
    }
}
